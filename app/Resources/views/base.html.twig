                            <li class="{% block questionsNav %}{% endblock%}"><a href="{{ path('personal_questions') }}">Questions</a></li>
                            <li class="{% block personalityNav %}{% endblock%}">
                            {% if not app.user.score %}
                                <a href="{{path('internjumb_quiz')}}">Personality Quiz</a>
                            {% else %}
                                    <a href="{{path('internjumb_quiz_page_result')}}">Personality Quiz</a>
                            {% endif %}
