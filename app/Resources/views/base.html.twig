                      <li class="{% block campusNav %}{% endblock%}"><a href="{{path('campus_resps')}}"><i class="fa fa-mobile-phone"></i> Campus Representatives</a></li>
                      {% endif %}

                    </ul>

                    {% if is_granted('ROLE_COMPANY') %}
                        <ul class="sub-nav">
                            <li class="{% block companyHomeNav %}{% endblock%}"><a href="{{ path('internship',{'loginName' : app.user.loginName}) }}">Home</a></li>
                            <li class="{% block newPosNav %}{% endblock%}"><a href="{{ path('internship_new') }}">Add new Position</a></li>
