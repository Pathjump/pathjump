                                 */
                                function updateCompanyNotifications(){
                                    $.ajax({
                                            type: 'POST',
                                            url: '{{path('get_company_notifications')}}',
                                            success: function(msg) {
                                                $('.notifsWrapper').html('');
                                                $('.notifsWrapper').html(msg);
                                            }
                                        });
                                }

