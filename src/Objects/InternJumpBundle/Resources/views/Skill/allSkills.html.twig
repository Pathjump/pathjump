{% extends '::base.html.twig' %}


{%block body%}
<h3>Student Skill list</h3>

<table class="records_list" border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ url('skill_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.title }}</td>
            <td>
                
                        <form action="{{ url('deleteSkill', { 'id': entity.id }) }}" method="post">
                                <button type="submit">Delete</button>
                        </form>
                        <form action="{{ url('skill_edit', { 'id': entity.id }) }}" method="post">
                            <button type="submit">Edit </button>
                        </form>

            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
            <form action="{{ url('skill_new') }}" method="POST"><input type="submit" Value="+"></form>

{%endblock%}