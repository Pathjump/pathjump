{% extends '::base.html.twig' %}


{%block body%}
<h3>Student Skill edit</h3>

<form action="{{ url('skill_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ url('skill') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ url('skill_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">-</button>
        </form>
    </li>
</ul>
{%endblock%}