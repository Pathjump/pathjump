{% extends '::fb_base.html.twig' %}
{% block title %}internjumb quiz Result{% endblock %}

{% block body %}

<div class="container Main-content">
    <div class="row-fluid show-grid">
        <div class="span12">
            <div class="company-form Resume">
                <div class="row-fluid Resume-title show-grid">
                    <div class="span10">
                        <h2>{{resultObject.message}}</h2>
                    </div>
                    <div class="span2 logo-box"><img src="{{resultObject.getTimThumbUrl(90,80)}}"></div>
                </div>
                <div class="Box-a">
                    <p>{{ resultObject.description | raw }}</p>

                </div>
                    <div class="Box-a" style="text-align: center">
                    <a href="{{path('fb_internjumb_quiz')}}" class="btn">Retake Quiz</a>
                    </div>
            </div>
        </div>
    </div>
</div>

{% endblock %}

{% block javascripts %}
{{parent()}}

{% endblock %}

